axis_0_pos = dpad_right
axis_1_neg = dpad_up
axis_1_pos = dpad_down
button_0 = west_button
button_1 = north_button
button_2 = south_button
button_3 = east_button
button_4 = right_shoulder
button_5 = right_trigger
button_6 = left_shoulder
button_7 = left_trigger
